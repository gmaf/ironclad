{
  "urls": "http://*:5005;",
  "authority": "http://localhost:5005",
  "issuerUri": "http://localhost:5005",
  "respectXForwardedForHeaders": true,
  "serilog": {
    "minimumLevel": {
      "default": "Debug",
      "override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "writeTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "RollingFile",
              "Args": { "pathFormat": "logs/ironclad/ironclad-developer-{Date}.log" }
            }
          ]
        }
      }
    ]
  },
  "Finbuckle:MultiTenant:InMemoryStore": {
    "DefaultConnectionString": "Datasource=sample.db",
    "TenantConfigurations": [
      {
        "Id": "tenant-ironclad",
        "Identifier": "ironclad",
        "Name": "Ironclad Test",
        "Items": 
        {
          "ChallengeScheme": "OpenIdConnect"
        }
      }
    ]
  }
}